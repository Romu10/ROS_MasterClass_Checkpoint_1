<?xml version="1.0" ?>

<robot name="robot">

    <!-- Gazebo Color's -->
    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="link_laser_scan">
        <material>Gazebo/Red</material>
    </gazebo>
    <gazebo reference="link_left_wheel">
        <material>Gazebo/Red</material>
    </gazebo>
    <gazebo reference="link_right_wheel">
        <material>Gazebo/Red</material>
    </gazebo>

    <!-- =========================================================================================================== -->

    <!-- Gazebo tags - Laser scan -->
    <gazebo reference="joint_laser_scan_chassis">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <gazebo reference="link_laser_scan">
        <material>Gazebo/DarkGrey</material>
    </gazebo>

    <!-- Laser work function -->

    <gazebo reference="link_laser_scan">
        <sensor type="ray" name="head_hokuyo_sensor">
            <pose>0 0 0 0 0 0</pose>
            <visualize>true</visualize>
            <update_rate>20</update_rate>
            <ray>
                <scan>
                    <horizontal>
                        <samples>720</samples>
                        <resolution>1</resolution>
                        <min_angle>-1.570796</min_angle>
                        <max_angle>1.570796</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.20</min>
                    <max>10.0</max>
                    <resolution>0.01</resolution>
                </range>
                <noise>
                    <type>gaussian</type>
                    <mean>0.0</mean>
                    <stddev>0.01</stddev>
                </noise>
            </ray>
            <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
                <topicName>/laser/scan</topicName>
                <frameName>sensor_laser</frameName>
            </plugin>
        </sensor>
    </gazebo>

    <!-- =========================================================================================================== -->

    <!-- Laser scan -->
    <joint name="joint_laser_scan_chassis" type="fixed">
        <origin rpy="0 0 0" xyz="0.25 0.0 0.075" />
        <child link="link_laser_scan" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <link name="link_laser_scan">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.5" />
            <inertia ixx="0.0002917" ixy="0" ixz="0" iyy="0.0002917" iyz="0" izz="0.000625"/>
        </inertial>
        <visual>ahor
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.05" length="0.10"/>
            </geometry>
            <material name="Red">
                <color rgba="0.0 1.0 0.0 0.8" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.05" length="0.10"/>
            </geometry>
        </collision>
    </link>

    <!-- =========================================================================================================== -->

    <!-- Link - Base Footprint -->
    <link name="base_footprint">`
        <inertial>
            <!-- Define la masa y el origen -->
            <mass value="0.0" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <!-- Define la inercia -->
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
        </inertial>

    </link>

    <!-- =========================================================================================================== -->

    <!-- Joint - Base Link -->
    <joint name="joint_base_link" type="fixed">
        <origin rpy="0 0 0" xyz="0.0 0.0 0.15" />
        <child link="base_link" />
        <parent link="base_footprint" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <link name="base_link">
        <inertial>
            <mass value="25" />
            <origin xyz="0 0 0" rpy="0 0 0.0" />
            <inertia ixx="0.01875" ixy="0" ixz="0" iyy="0.01875" iyz="0" izz="0.03125" />
        </inertial>
        
        <collision>
            <geometry>
                <cylinder radius="0.25" length="0.3" />
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder radius="0.25" length="0.3" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

    </link>

    <!-- =========================================================================================================== -->

    <!-- Joint - Front Caster wheel -->
    <joint name="joint_front_chassis_caster_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0.0 -0.15" />
        <child link="link_front_caster_wheel" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <!-- Link - Front Caster wheel -->
    <link name="link_front_caster_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0.1 0.0 -0.15" rpy="0 0 0" />
            <inertia ixx="0.000125" ixy="0" ixz="0" iyy="0.000125" iyz="0" izz="0.000125"/>
        </inertial>

        <!-- Front wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.01" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.01" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Front Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.01" />
            <geometry>
                <sphere radius="0.010" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.01" />
            <geometry>
                <sphere radius="0.010" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- =========================================================================================================== -->

    <!-- Joint - Back caster wheel -->
    <joint name="joint_back_chassis_caster_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0.0 -0.15" />
        <child link="link_back_caster_wheel" />
        <parent link="base_link" />
        <joint_properties damping="1.0" friction="0.0" />
    </joint>

    <!-- Link - Back Caster Wheel -->
    <link name="link_back_caster_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="-0.1 0.0 -0.15" rpy="0 0 0" />
            <inertia ixx="0.000125" ixy="0" ixz="0" iyy="0.000125" iyz="0" izz="0.000125"/>
        </inertial>

        <!-- Back wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.01" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.01" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Back Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.01" />
            <geometry>
                <sphere radius="0.010" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.01" />
            <geometry>
                <sphere radius="0.010" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- =========================================================================================================== -->

    <!-- Joint - chassis / left wheel -->
    <joint name="joint_chassis_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 0.2 -0.15" />
        <child link="link_left_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <!-- Link - left wheel -->
    <link name="link_left_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0.0 0.2 -0.15" rpy="0 0 0" />
            <inertia ixx="0.00001041667" ixy="0" ixz="0" iyy="0.00001041667" iyz="0" izz="0.0000015625"/>
        </inertial>

        <!-- Tires -->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.03" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.03" radius="0.025"/>
            </geometry>
            <material name="red">
                <color rgba="1 0.0 0.0 1" />
            </material>
        </visual>

    </link>

    <!-- =========================================================================================================== -->

    <!-- Joint - chassis / right wheel -->
    <joint name="joint_chassis_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 -0.2 -0.15" />
        <child link="link_right_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>
    <!-- Link - right wheel -->
    <link name="link_right_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0.0 -0.2 -0.15" rpy="0 0 0" />
            <inertia ixx="0.00001041667" ixy="0" ixz="0" iyy="0.00001041667" iyz="0" izz="0.0000015625"/>
        </inertial>

        <!-- Tires -->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.03" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.03" radius="0.025"/>
            </geometry>
            <material name="red">
                <color rgba="1 0.0 0.0 1" />
            </material>
        </visual>

    </link>
</robot>

<!-- =========================================================================================================== -->
